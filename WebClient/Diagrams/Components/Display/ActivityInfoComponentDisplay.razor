<div class="card">
    <div class="card-body">
        <h4>Add activities that can be done there</h4>
        @foreach (var item in Node.Snorkel.ActivityInfos)
        {
            <p>@item.ActivityName</p>
        }
    </div>
</div>

@code{
    [Parameter]
    public ActivityInfoNodeDisplay Node { get; set; }
}
